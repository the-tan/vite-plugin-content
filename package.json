{
  "name": "vite-plugin-content",
  "license": "MIT",
  "version": "0.1.3",
  "author": "the-tan",
  "repository": {
    "type": "git",
    "url": "https://github.com/the-tan/vite-plugin-content"
  },
  "type": "module",
  "bin": {
    "vite-plugin-content": "./dist/cli/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./remark-plugins/*": {
      "import": "./dist/remark-plugins/index.mjs",
      "require": "./dist/remark-plugins/index.cjs",
      "types": "./dist/remark-plugins/index.d.ts"
    },
    "./cli/*": {
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.cjs",
      "types": "./dist/cli/index.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf dist && unbuild && tsx scripts/move-tsconfig.ts",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && npm publish"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/micromatch": "^4.0.2",
    "@types/mustache": "^4.2.2",
    "@types/node": "^20.6.0",
    "bumpp": "^9.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.1",
    "tslib": "^2.6.2",
    "tsx": "^3.12.10",
    "typescript": "^5.0.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.5"
  },
  "peerDependencies": {
    "vite": ">=3.1.0"
  },
  "dependencies": {
    "bundle-require": "^4.0.1",
    "camelcase": "^8.0.0",
    "clipanion": "4.0.0-rc.2",
    "del": "^7.1.0",
    "fs-extra": "^11.1.1",
    "github-slugger": "^2.0.0",
    "glob": "^10.3.4",
    "image-size": "^1.0.2",
    "joycon": "^3.1.1",
    "mdast-util-to-string": "^4.0.0",
    "mdx-bundler": "^9.2.1",
    "micromatch": "^4.0.5",
    "mustache": "^4.2.0",
    "unified": "10.1.2",
    "unist-util-visit": "^5.0.0",
    "zod": "^3.22.2"
  }
}
